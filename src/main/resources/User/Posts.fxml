<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="700.0" prefWidth="1200.0" stylesheets="@../style.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="demo.controllers.UserPoste">
   <children>
      <ScrollPane fx:id="ScrollePan" layoutX="320.0" layoutY="140.0" prefHeight="569.0" prefWidth="878.0" />
      <ImageView fitHeight="73.0" fitWidth="67.0" layoutX="10.0" layoutY="11.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/jimmy-fallon.png" />
         </image>
         <VBox.margin>
            <Insets bottom="10.0" top="50.0" />
         </VBox.margin>
      </ImageView>
      <Label layoutX="102.0" layoutY="83.0" text="Jimmy Fallon" textFill="#130303">
         <VBox.margin>
            <Insets bottom="20.0" />
         </VBox.margin>
      </Label>
      <!-- Buttons and separators go here -->
      <Button layoutX="908.0" layoutY="91.0" mnemonicParsing="false" onAction="#loaAllPoste" text="all posts" />
      <Button layoutX="980.0" layoutY="91.0" mnemonicParsing="false" onAction="#loaUserPoste" text="my posts" />
      <Button layoutX="1064.0" layoutY="91.0" mnemonicParsing="false" onAction="#goAddPost" prefHeight="26.0" prefWidth="78.0" text="Add Post" />
      <TextField fx:id="recherchefld" layoutX="490.0" layoutY="91.0" prefWidth="328.0" promptText="rechercher" />
   </children>
</AnchorPane>
